import java.util.Scanner;

public class code1 {

	public static void main(String[] args) {

		
		Scanner sc = new Scanner(System.in);
		
		System.out.println("숫자 입력");
		
		
		
		int input = sc.nextInt();
		
		int sum = 0;
		
		while(input>0){
			sum += input%10;
			input /= 10;
		}

		
		System.out.println(sum);

	}

}
